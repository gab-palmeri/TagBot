{
   "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext"],
      "module": "ESNext",
      "moduleResolution": "bundler",
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "baseUrl": "./src"
   },
   "exclude": ["node_modules", "dist"]
}
